<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzc.lease.web.admin.mapper.CityInfoMapper">

    <select id="selectPNameByID" resultType="java.lang.String">
        select name
        from city_info
        where id = #{cityId}
    </select>
</mapper>
